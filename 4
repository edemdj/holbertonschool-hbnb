class Country:
    def __init__(self, country_id: int, name: str):
self.country_id = country_id
        self.name = name
        self.cities = []  # Liste pour stocker les objets City

    def add_city(self, city):
        """Ajoute une ville Ã  la liste des villes du pays."""
        self.cities.append(city)

    def get_cities(self):
        """Retourne la liste des villes du pays."""
        return self.cities

    def __str__(self):
        return f"Country({self.country_id}, {self.name})"

